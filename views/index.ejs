<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link href="/css/style.css" rel="stylesheet" type="text/css" />
    <title>Index page</title>
  </head>
  <body>
    <div class="container">
      <div class="row">
        <div class="col-md-4"></div>
        <div class="col-md-4 d-flex justify-content-center">
          <form action="/login" method="POST" class="card-white">
            <div class="CardHeader-cardHeaderText-481">
              <h4 class="Connect\(LogIn\)-cardTitle-22">Login</h4>
            </div>
            <div class="CardBody-cardBody-100">
              <p class="text-muted">
                Please enter your agri dealer login and password that you want
                to compare!
              </p>
              <input type="email" name="email" placeholder="Enter email" style="margin-bottom: 20px; width: 100%" />
              </br>
              <input type="password" name="password" placeholder="Password"  style="margin-bottom: 20px; width: 100%"/>
              <div class="d-flex align-items-center">
                <label for="IsBefore" style="color: #6c757d !important"
                  >Before Deploye</label
                >
                <input
                  type="checkbox"
                  id="IsBefore"
                  name="IsBefore"
                  value="true"
                  style="margin-left: 15px; width: 20px"
                />
              </div>
            </div>
            <div class="CardFooter-cardFooter-122">
              <button class="RegularButton-primary-220" style="width: 100%; margin-right:0" type="submit">
                Submit
              </button>
            </div>
          </form>
        </div>
        <div class="col-md-4"></div>   
      </div>
    </div>
  </body>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"
  ></script>
</html>
