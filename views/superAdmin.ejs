<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link href="https://unpkg.com/jquery.json-viewer@1.4.0/json-viewer/jquery.json-viewer.css" type="text/css"
    rel="stylesheet" />
    <link href="/css/style.css" rel="stylesheet" type="text/css" />
    <title>Index page</title>
  </head>
  <body>
    <div class="container">
        <div class="d-flex justify-content-between mt-3">
            <h1 class="CustomersList-cardIconTitle-347">All Oraganization Data</h1>

                <button class="RegularButton-primary-220 ">
                    <a href="/login"> Back to Testing page </a>
                </button>
      </div>
      <div class="row">
        <div class="d-flex justify-content-center">
          

            <% if (data.length===0) { %>
                <div class="card-white">
                    <div class="CustomersList-cardHeaderContent-349 justify-content-center">
                        <h5 class="CustomersList-cardIconTitle-347">All Organization Data</h5>
                    </div>
                </div>
        
                <%} else { %>
                    <div class="card-white">
        
                        <table class="table table-striped table-class" >
                            <thead>
                                <tr>
                                    <th>Organization</th>
                                    <th>Email</th>
                                    <th>Address</th>
                                    <th>Phone Number</th>
                                    <th>Bayer C / N</th>
                                    <th>Compaines</th>

                                    <th>Seed Compaines</th>
                                    <th> Customers</th>

                                    <th>Purchase Orders</th>

                                    <th>Quotes</th>
                                    <th>Discounts</th>

        
                                </tr>
                            </thead>
                            <tbody>
                                <% for(var i=0; i<data.length; i++) {%>
                                    <tr>
                                        <td>
                                            <%= data[i].name %>
                                        </td>
        
                                        <td>
                                            <%= data[i].email %>
                                        </td>
                                        <td>
                                            <%= data[i].address %>
                                        </td>
                                        <td>
                                            <%= data[i].phoneNumber %>
                                        </td>
                                        <td>
                                            <%= data[i].apiSeedCompanies %>
                                        </td> <td>
                                            <%= data[i].companies %>
                                        </td> <td>
                                            <%= data[i].seedCompanies %>
                                        </td>
                                        <td>
                                            <%= data[i].customers %>
                                        </td>
                                        <td>
                                            <%= data[i].purchaseOrders %>
                                        </td>
                                        <td>
                                            <%= data[i].quotes %>
                                        </td>
                                        <td>
                                            <%= data[i].discounts %>
                                        </td>
                                    </tr>
                                    <% } %>
                            </tbody>
                        </table>
                       
                    </div>
                    </div>
                    <% } %>


        </div>
       
      </div>
    </div>
  </body>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"
  ></script>
</html>
