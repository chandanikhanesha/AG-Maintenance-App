<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Index page</title>
    <script
      src="https://code.jquery.com/jquery-3.6.0.slim.min.js"
      integrity="sha256-u7e5khyithlIdTpu22PHhENmPcRdFiHRjhAuHcs05RI="
      crossorigin="anonymous"
    ></script>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <script src="https://unpkg.com/jquery.json-viewer@1.4.0/json-viewer/jquery.json-viewer.js"></script>
    <link
      href="https://unpkg.com/jquery.json-viewer@1.4.0/json-viewer/jquery.json-viewer.css"
      type="text/css"
      rel="stylesheet"
    />
    <link href="/css/style.css" rel="stylesheet" type="text/css" />

    <script>
    
    </script>
  </head>
  <body>
    <div class="container">
      <div class="d-flex justify-content-center mt-3">
        <h1  class="CustomersList-cardIconTitle-347">Crashed Purchase Order</h1>
      </div>
      <pre id="json-renderer"></pre>
      <% if (data.length===0) { %>
        <div class="card-white">
          <div class="CustomersList-cardHeaderContent-349 justify-content-center">
            <h5 class="CustomersList-cardIconTitle-347">No Crashed Purchase Order</h5>
            </div>
            </div>
            <%} else { %>
              <div class="CustomersList-cardHeaderContent-349">
                <h3 class="CustomersList-cardIconTitle-347">Error Message of Testing</h3>
              </div>
      
              <div class="card-white">
                
      <table class="table">
        <thead>
          <tr>
            <th>OrgId</th>
            <th>Purchase Order No.</th>
            <!-- <th>CustomerMonsantoProductCounts</th>
            <th>monsantoProductCounts</th>
           <th>monsantoProductLineItemCounts</th> -->
           <th>missingMonsantoProducts</th>
           <th>missingMonsantoProductLineItems</th>

          </tr>
        </thead>
        <tbody>
          <% for(var i=0; i<data.length; i++) {%>
          <tr>
            
            <td><%= data[i].org %></td>
           <td><%= data[i].purchaseOrder %></td>
           <!-- <td><%= data[i].CustomerMonsantoProductCounts %></td>
           <td><%= data[i].monsantoProductCounts %></td>
           <td><%= data[i].monsantoProductLineItemCounts %></td> -->
           <td><%= data[i].missingMonsantoProducts %></td>
           <td><%= data[i].missingMonsantoProductLineItems %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </div>
    <% } %>
    </div>
    <div class="d-flex justify-content-center">
      <button class="RegularButton-primary-220 ">
        <a href="/login"> Back to Testing page </a>
      </button>
    </div>
  </body>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"
  ></script>
</html>
