<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Index page</title>
    <script src="https://code.jquery.com/jquery-3.6.0.slim.min.js"
        integrity="sha256-u7e5khyithlIdTpu22PHhENmPcRdFiHRjhAuHcs05RI=" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
    <script src="https://unpkg.com/jquery.json-viewer@1.4.0/json-viewer/jquery.json-viewer.js"></script>
    <link href="https://unpkg.com/jquery.json-viewer@1.4.0/json-viewer/jquery.json-viewer.css" type="text/css"
        rel="stylesheet" />
    <link href="/css/style.css" rel="stylesheet" type="text/css" />



</head>

<body>

    <div class="container" style="max-width:1650px">
        <div class="d-flex justify-content-center mt-3">
            <h1 class="CustomersList-cardIconTitle-347">List Zones LOGS</h1>
        </div>
        <pre id="json-renderer"></pre>
       
    </div>

    <% if (data.length===0) { %>
        <div class="card-white">
            <div class="CustomersList-cardHeaderContent-349 justify-content-center">
                <h5 class="CustomersList-cardIconTitle-347">No Logs Created</h5>
            </div>
        </div>

        <%} else { %>
            <div class="card-white">

                <table class="table table-striped table-class" id="table-id">
                    <thead>
                        <tr>
                            <th>OrgID</th>
                            <th>OrdName</th>
              
                            <th>CustomerTableZoneIds</th>
                            <th>ApiTableZoneIds</th>
                            <th>BayerZoneId</th>


                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0; i<data.length; i++) {%>
                            <tr>
                                <td>
                                    <%= data[i].organizationId %>
                                </td>

                                <td>
                                    <%= data[i].orgName %>
                                </td>
                             
                                <td>
                                    <%= JSON.stringify(data[i].customerZoneIds) %>
                                </td>
                                <td>
                                    <%= JSON.stringify(data[i].apiZoneIds) %>
                                </td>
                                <td>
                                    <%= JSON.stringify(data[i].bayerZoneId) %>
                                </td>
                            </tr>
                            <% } %>
                    </tbody>
                </table>
               
            </div>
            </div>
            <% } %>
                </div>
               
</body>


</html>